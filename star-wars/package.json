{
  "private": true,
  "scripts": {
    "start-server": "nodemon ./server.js  --exec babel-node",
    "start-server-v2": "nodemon ./server.js  --exec babel-node  --watch data --ignore  data/schema.json --ignore data/schema.graphql",
    "debug-server": "babel-node ./server.js --source-maps --debug-brk --inspect",
    "debug-server-v2": "node  ./node_modules/babel-cli/bin/babel-node.js  ./server.js --debug-brk --inspect",
    "start-client": "webpack-dev-server --port 3000 --inline",
    "update-schema": "babel-node ./script/updateSchema.js",
    "start": "npm-run-all --parallel start-server   start-client"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "0.9.2",
    "babel-runtime": "^6.11.6",
    "classnames": "2.2.5",
    "express": "4.14.0",
    "express-graphql": "0.5.3",
    "graphql": "0.6.2",
    "graphql-relay": "0.4.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-relay": "0.9.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.11.4"
  }
}
